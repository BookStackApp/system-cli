{
    "require": {
        "ext-zip": "*",
        "symfony/console": "^6.0",
        "symfony/process": "^6.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "ext-mysqli": "*"
    }
}
